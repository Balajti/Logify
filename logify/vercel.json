{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true,
      "feature/**": true
    }
  },
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true
  },
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ./",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ]
}